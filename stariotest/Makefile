####### Compiler, tools and options

CC       = gcc
CXX      = g++
LEX      = flex
YACC     = yacc
CFLAGS   = -pipe -Wall -W -O2 -fomit-frame-pointer -pipe -march=i586 -mcpu=pentiumpro -D_REENTRANT  -DQT_NO_DEBUG -DQT_THREAD_SUPPORT
CXXFLAGS = -pipe -Wall -W -O2 -fomit-frame-pointer -pipe -march=i586 -mcpu=pentiumpro -D_REENTRANT  -DQT_NO_DEBUG -DQT_THREAD_SUPPORT
LEXFLAGS = 
YACCFLAGS= -d
INCPATH  = -I$(QTDIR)/mkspecs/default -I. -I$(QTDIR)/include -I.ui/ -I.moc/
LINK     = g++
LFLAGS   = 
LIBS     = $(SUBLIBS) -L$(QTDIR)/lib -L/usr/X11R6/lib -lstario -lqt-mt -lXext -lX11 -lm -lpthread
AR       = ar cqs
RANLIB   = 
MOC      = $(QTDIR)/bin/moc
UIC      = $(QTDIR)/bin/uic
QMAKE    = qmake
TAR      = tar -cf
GZIP     = gzip -9f
COPY     = cp -f
COPY_FILE= $(COPY) -p
COPY_DIR = $(COPY) -pR
DEL_FILE = rm -f
SYMLINK  = ln -sf
DEL_DIR  = rmdir
MOVE     = mv -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p

####### Output directory

OBJECTS_DIR = .obj/

####### Files

HEADERS = 
SOURCES = main.cpp
OBJECTS = .obj/main.o \
		.obj/stariotest.o
FORMS = stariotest.ui
UICDECLS = .ui/stariotest.h
UICIMPLS = .ui/stariotest.cpp
SRCMOC   = .moc/moc_stariotest.cpp
OBJMOC = .obj/moc_stariotest.o
DIST	   = stariotest.pro
QMAKE_TARGET = stariotest
DESTDIR  = 
TARGET   = stariotest

first: all
####### Implicit rules

.SUFFIXES: .c .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules

all: Makefile $(TARGET)

$(TARGET): $(UICDECLS) $(OBJECTS) $(OBJMOC) 
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(LIBS)

mocables: $(SRCMOC)

$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: stariotest.pro  $(QTDIR)/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile stariotest.pro
qmake: 
	@$(QMAKE) -o Makefile stariotest.pro

dist: 
	@mkdir -p .obj/stariotest && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/stariotest/ && $(COPY_FILE) --parents stariotest.ui.h .obj/stariotest/ && ( cd `dirname .obj/stariotest` && $(TAR) stariotest.tar stariotest && $(GZIP) stariotest.tar ) && $(MOVE) `dirname .obj/stariotest`/stariotest.tar.gz . && $(DEL_FILE) -r .obj/stariotest

mocclean:
	-$(DEL_FILE) $(OBJMOC)
	-$(DEL_FILE) $(SRCMOC)

uiclean:
	-$(DEL_FILE) $(UICIMPLS) $(UICDECLS)

yaccclean:
lexclean:
clean: mocclean uiclean
	-$(DEL_FILE) $(OBJECTS) 
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

distclean: clean
	-$(DEL_FILE) $(TARGET) $(TARGET)


FORCE:

####### Compile

.obj/main.o: main.cpp .ui/stariotest.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/main.o main.cpp

.ui/stariotest.h: stariotest.ui 
	$(UIC) stariotest.ui -o .ui/stariotest.h

.ui/stariotest.cpp: .ui/stariotest.h stariotest.ui 
	$(UIC) stariotest.ui -i stariotest.h -o .ui/stariotest.cpp

.obj/stariotest.o: .ui/stariotest.cpp stariotest.ui.h \
		.ui/stariotest.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/stariotest.o .ui/stariotest.cpp

.obj/moc_stariotest.o: .moc/moc_stariotest.cpp .ui/stariotest.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_stariotest.o .moc/moc_stariotest.cpp

.moc/moc_stariotest.cpp: $(MOC) .ui/stariotest.h
	$(MOC) .ui/stariotest.h -o .moc/moc_stariotest.cpp

####### Install

install: all 

uninstall: 

